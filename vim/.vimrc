set shortmess=I

set number
set numberwidth=1
hi LineNr ctermfg=8

set laststatus=2
"hi StatusLine
"    \ guibg=White ctermfg=8
"    \ guifg=DarkSlateGrey ctermbg=15
set statusline=
set statusline+=\%f "filename
set statusline+=\%m "modified
set statusline+=%= "alignment
set statusline+=\%l:%c "line:col

